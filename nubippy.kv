<Widget>:
	font_name:'res/fonts/Roboto-Medium.ttf'

<TopActionBar>:

	id: topActionBar

	ActionView:
	    id: topActionView

	    ActionPrevious:
	        id: topActionPrevious
	        with_previous: True
	        app_icon: 'res/icons/nu.png'
	        title: app.get_string('Main_Title')
	        on_release: root.reset_ui()
	        background_down: 'res/images/branding/red.png'

